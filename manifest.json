{
  "manifest_version": 2,
  "name": "Discord Shh",
  "version": "0.1",
  "background": {
    "scripts": ["dist/background.js"]
  },
  "permissions": [
    "webRequest",
    "tabs",
    "*://*.discordapp.com/",
    "*://*.discordapp.net/"
  ],
  "browser_action": {
    "default_icon": "icons/icon32dull.png"
  }
}
